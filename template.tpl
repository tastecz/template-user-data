___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const getType = require('getType');
const injectScript = require('injectScript');
const isConsentGranted = require('isConsentGranted');
const setCookie = require('setCookie');
const copyFromWindow = require('copyFromWindow');
const localStorage = require('localStorage');
const JSON = require('JSON');
const createQueue = require('createQueue');
const dataLayerPush = createQueue('dataLayer');
const getCookie = require('getCookieValues');
const makeInteger = require('makeInteger');
const gtmOnSuccess = data.gtmOnSuccess;
const gtmOnFailure = data.gtmOnFailure;
const formSelector = data.formSelector;
const checkboxLocalStorage = data.checkboxLocalStorage;
const checkboxSessionStorage = data.checkboxSessionStorage;
const checkboxCookie = data.checkboxCookie;
const cookieDuration = makeInteger(data.cookieDuration);
const checkboxDataLayer = data.checkboxDataLayer;
const replaceValues = data.replaceValues;
const defaultName = 'user_data';

// Local Storage function
const createLocalStorage = (ud) => {
    // Replace whole object
    if (replaceValues === 'replace') {
        localStorage.setItem(defaultName, ud);
    // Merge old and new data
    } else if (replaceValues === 'merge') {
        let oldObject = localStorage.getItem(defaultName);
        if (oldObject) {
            oldObject = JSON.parse(oldObject);
            let newObject = JSON.parse(ud);
            for (let key in newObject) {
                if (oldObject.hasOwnProperty(key)) oldObject[key] = newObject[key];
            }
            localStorage.setItem(defaultName, JSON.stringify(oldObject));
        } else {
            localStorage.setItem(defaultName, ud);
        }
    }
};

// Session Storage function
const createSessionStorage = (_ta, ud) => {
    // Replace whole object
    if (replaceValues === 'replace') {
        _ta('session_storage', 'set', defaultName, ud);
    // Merge old and new data
    } else if (replaceValues === 'merge') {
        let oldObject = _ta('session_storage', 'get', defaultName);
        if (oldObject) {
            oldObject = JSON.parse(oldObject);
            let newObject = JSON.parse(ud);
            for (let key in newObject) {
                if (oldObject.hasOwnProperty(key)) oldObject[key] = newObject[key];
            }
            _ta('session_storage', 'set', defaultName, JSON.stringify(oldObject));
        } else {
            _ta('session_storage', 'set', defaultName, ud);
        }
    }
};

// Cookie function
const createCookie = (ud) => {
    const cookieOptions = {
        'path': '/',
        'max-age': 60 * 60 * 24 * cookieDuration
    };
    // Replace whole object
    if (replaceValues === 'replace') {
        setCookie(defaultName, ud, cookieOptions);
    // Merge old and new data
    } else if (replaceValues === 'merge') {
        let oldObject = getCookie(defaultName);
        if (oldObject && oldObject.length !== 0) {
            oldObject = JSON.parse(oldObject[0]);
            let newObject = JSON.parse(ud);
            for (let key in newObject) {
                if (oldObject.hasOwnProperty(key)) oldObject[key] = newObject[key];
            }
            setCookie(defaultName, JSON.stringify(oldObject), cookieOptions);
        } else {
            setCookie(defaultName, ud, cookieOptions);
        }
    }
};

// DataLayer event function
const sendEvent = (ud) => {
    const object = {
        'event': 'user_data_saved',
        'user': JSON.parse(ud)
    };
    dataLayerPush(object);
};

// Extracts the user data from a form inputs
const getUserData = (_ta) => {
    let tableUserParams = data.tableUserParameters;
    let inputNames = [];
    tableUserParams.forEach(param => {
        inputNames.push([param.userParameter, param.userValue]);
    });
    if (inputNames.length > 0) {
        let userData = _ta('user_data', inputNames, formSelector);
        return userData;
    } else {
        gtmOnFailure();
    }
};

// Main function that checks the checkboxes and saves the user data
const saveUserData = (_ta, ud) => {
    if (!ud) return;
    if (getType(ud) === 'object') ud = JSON.stringify(ud);

    // Event to dataLayer doesn't need consent
    if (checkboxDataLayer) sendEvent(ud);

    // Checks the user's consent
    if (isConsentGranted('ad_user_data')) {
        if (checkboxCookie) createCookie(ud);
        if (checkboxLocalStorage) createLocalStorage(ud);
        if (checkboxSessionStorage) createSessionStorage(_ta, ud);
    }
};

// Function that fetches external script and loads a javascript library
const getLibrary = () => {
    const _ta = copyFromWindow('__taste');
    let userData = getUserData(_ta);
    saveUserData(_ta, userData);
};

// Inject the external script asynchronously
const scriptUrl = 'https://taste.cz/analytics/gtm-template.js';
injectScript(scriptUrl, getLibrary, gtmOnFailure, 'tasteUserData');


